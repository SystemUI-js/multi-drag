{
  "name": "@system-ui-js/multi-drag",
  "version": "0.2.6-dev.2",
  "description": "A multi-drag library.",
  "keywords": [
    "touch",
    "multi-drag",
    "drag",
    "system-ui"
  ],
  "author": "ZX",
  "license": "MIT",
  "homepage": "https://systemui-js.github.io/multi-drag/",
  "repository": {
    "type": "git",
    "url": "https://github.com/SystemUI-js/multi-drag"
  },
  "bugs": {
    "url": "https://github.com/SystemUI-js/multi-drag/issues"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --project tsconfig.build.json",
    "build:lib": "vite build --mode lib",
    "build:demo": "vite build --mode demo",
    "build:all": "npm run build:lib && npm run build:demo && npm run build:types",
    "build:types": "tsc --project tsconfig.build.json",
    "preview": "vite preview",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:basic": "playwright test tests/e2e/multi-touch.spec.ts --project=chromium",
    "test:e2e:report": "playwright show-report",
    "prepublishOnly": "npm run test && npm run build:all",
    "publish:npm": "npm publish",
    "release": "npm run prepublishOnly && npm run publish:npm"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@system-ui-js/development-base": "^0.0.4",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "playwright": "^1.54.2",
    "prettier": "^3.7.4",
    "prettier-config-standard": "^7.0.0",
    "ts-jest": "^29.4.1",
    "tslint": "^6.1.3",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^5.0.2",
    "typescript-eslint": "^8.49.0",
    "vconsole": "^3.15.1",
    "vite": "^4.4.5",
    "vite-plugin-checker": "^0.6.2"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "loglevel": "^1.9.2",
    "mathjs": "^14.6.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "package.json",
    "LICENSE",
    "assets"
  ],
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
